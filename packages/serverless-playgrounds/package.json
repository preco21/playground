{
  "name": "serverless-playgrounds",
  "version": "0.0.0",
  "description": "Yet another minimal setup for Serverless app",
  "author": "Plusb Preco <plusb21@gmail.com>",
  "private": true,
  "bin": {
    "custom-env": "bin/custom-env.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "serverless offline start",
    "admin": "dynamodb-admin",
    "setup": "npm run dynamodb:setup",
    "remove": "npm run dynamodb:remove",
    "dynamodb:setup": "serverless dynamodb install",
    "dynamodb:remove": "serverless dynamodb remove",
    "dynamodb:start": "serverless dynamodb start",
    "custom-script": "custom-env node scripts/custom-script.js"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "aws-sdk": "^2.437.0",
    "core-js": "^3.0.1",
    "is-plain-obj": "^1.1.0",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@zeit/webpack-asset-relocator-loader": "^0.3.7",
    "babel-loader": "^8.0.5",
    "dotenv": "^7.0.0",
    "dotenv-webpack": "^1.7.0",
    "dynamodb-admin": "^3.1.3",
    "execa": "^1.0.0",
    "serverless": "^1.40.0",
    "serverless-dynamodb-local": "^0.2.37",
    "serverless-offline": "^4.9.4",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.30.0",
    "webpackbar": "^3.1.5"
  },
  "externals": [
    "aws-sdk"
  ],
  "workspaces": {
    "nohoist": [
      "**"
    ]
  }
}
