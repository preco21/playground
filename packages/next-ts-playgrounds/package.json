    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/styled-components": "^4.1.19",
    "@zeit/next-css": "^1.0.1",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "^6.0.3",
    "dotenv-webpack": "^1.7.0",
    "next": "^9.1.1",
