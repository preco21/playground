{
  "name": "next-ts-playgrounds",
  "version": "0.0.0",
  "description": "Setup for Next.js typescript application",
  "author": "Plusb Preco <plusb21@gmail.com>",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development next src",
