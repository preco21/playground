{
  "name": "electron-playgrounds",
  "productName": "Electron Playgrounds",
  "version": "0.0.0",
  "description": "Setup for Electron application",
  "author": "Plusb Preco <plusb21@gmail.com>",
  "private": true,
  "main": ".out/index.js",
  "scripts": {
    "postinstall": "electron-rebuild",
    "prepackage": "yarn run build",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node scripts/serve.js",
    "build": "npm-run-all build:main build:renderer",
    "build:main": "webpack --mode production",
    "build:renderer": "next build renderer && next export --outdir .out/renderer renderer",
    "package": "node scripts/package.js"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "electron-unhandled": "^2.2.0",
    "modern-normalize": "^0.5.0",
    "nanoid": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "source-map-support": "^0.5.9",
    "styled-components": "^4.1.3",
    "tempy": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@zeit/next-css": "^1.0.1",
    "@zeit/webpack-asset-relocator-loader": "^0.5.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.1",
    "clean-webpack-plugin": "^3.0.0",
    "cpy": "^7.3.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "^5.0.4",
    "electron-builder": "^20.43.0",
    "electron-debug": "^3.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.4",
    "execa": "^0.10.0",
    "exit-hook": "^2.0.0",
    "file-loader": "^2.0.0",
    "next": "^8.0.2",
    "next-compose-plugins": "^2.1.1",
    "next-fonts": "^0.16.0",
    "next-images": "^1.0.4",
    "next-plugin-custom-babel-config": "^1.0.2",
    "npm-run-all": "^4.1.2",
    "tree-kill": "^1.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "webpackbar": "^3.1.3",
    "write-pkg": "^3.1.0"
  },
  "build": {
    "appId": "com.electron.playgrounds",
    "directories": {
      "buildResources": "resources"
    }
  },
  "resources": [
    "static"
  ],
  "externals": [
    "electron-debug",
    "electron-devtools-installer"
  ],
  "eslintConfig": {
    "globals": {
      "ipc": false
    }
  }
}
