{
  "name": "electron-playgrounds",
  "productName": "Electron Playgrounds",
  "version": "0.0.0",
  "description": "Yet another opinionated setup for Electron application",
  "author": "Plusb Preco <plusb21@gmail.com>",
  "private": true,
  "main": ".app/index.js",
  "scripts": {
    "prepackage": "yarn run build",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node scripts/serve.js",
    "build": "cross-env NODE_ENV=production npm-run-all build:main build:renderer",
    "build:main": "webpack --mode production",
    "build:renderer": "next build renderer && next export --outdir .app/renderer renderer",
    "package": "node scripts/package.js"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "electron-is-dev": "^0.3.0",
    "modern-normalize": "^0.5.0",
    "nanoid": "^1.0.4",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "source-map-support": "^0.5.9",
    "styled-components": "^3.4.9",
    "tempy": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@zeit/next-css": "^1.0.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-styled-components": "^1.5.1",
    "clean-webpack-plugin": "^1.0.0",
    "cpy": "^7.0.1",
    "cross-env": "^5.2.0",
    "dotenv-webpack": "^1.5.7",
    "electron": "^4.0.0",
    "electron-builder": "^20.13.4",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "execa": "^0.10.0",
    "exit-hook": "^2.0.0",
    "file-loader": "^2.0.0",
    "next": "^7.0.2",
    "next-compose-plugins": "^2.1.1",
    "next-fonts": "^0.16.0",
    "npm-run-all": "^4.1.2",
    "size-plugin": "^1.0.1",
    "terser-webpack-plugin": "^1.1.0",
    "tree-kill": "^1.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-stats-plugin": "^0.2.1",
    "webpackbar": "^3.1.3",
    "write-pkg": "^3.1.0"
  },
  "app": {
    "appDest": ".app",
    "mainSource": "main",
    "rendererSource": "renderer",
    "devServerPort": 3000,
    "cleanPaths": [
      ".app"
    ],
    "externalsWhitelist": [
      "@babel/polyfill",
      "core-js",
      "regenerator-runtime"
    ]
  },
  "build": {
    "appId": "com.electron.playgrounds",
    "directories": {
      "buildResources": "resources",
      "output": "build"
    }
  }
}
