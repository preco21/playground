    "cpy-cli": "^2.0.0",
    "dotenv-webpack": "^1.5.7",
    "next": "^8.0.2",
    "next-compose-plugins": "^2.1.1",
    "next-fonts": "^0.17.0",
    "next-images": "^1.0.4",
    "npm-run-all": "^4.1.3"
